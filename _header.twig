<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
      {% if record.title is defined %}{{ record.title|striptags }} | {% endif %}
      {{ app.config.get('general/sitename') }}
      {% if record.title is not defined and app.config.get('general/payoff') %} | {{ app.config.get('general/payoff') }}{% endif %}
    </title>
    <link rel="stylesheet" href="{{ paths.theme }}stylesheets/app.css">
    <link rel="stylesheet" href="{{ paths.theme }}stylesheets/override.css">
    <link rel="stylesheet" href="{{ paths.theme }}stylesheets/videoResponsive.css">
    {% if 'rssfeed' %}<link rel="alternate" type="application/rss" href="/rss/feed.rss" />{% endif %}
    <script src="{{ paths.theme }}javascripts/jquery.min.js"></script>
  </head>
  <body>

    <div class="full wrapper hi">
    {% include '_menu.twig' %}

      <div class="full row">

        <div id="header" class="large-6 medium-4 small-12 columns hi">
          <div class="show-for-medium-up row spacer8"></div>
          <div class="show-for-large-up row spacer10"></div>
          <div class="row">
            <div class="small-12 medium-8 medium-offset-4 large-7 large-offset-5 columns">
              <h1 class="small-only-text-center"><a href="/">{{ app.config.get('general/sitename') }}</a></h1>
              <h3 class="small-only-text-center">{{ app.config.get('general/payoff') }}</h3>
            </div>
          </div>
        </div><!--header-->

        <div  id="content" class="large-6 medium-8 small-12 columns">
          <div class="row">
            <div class="small-12 large-10 large-centered columns">
              <div class="show-for-medium-up row spacer5"></div>
              <div class="show-for-large-up row spacer8"></div>

